<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!--Chuyển từ demo data thành master data để thêm mới dữ liệu mà không cần gỡ cài đặt module -->
    <data noupdate="1">
        <record id="estate.demo_estate_property_1" model="estate.property">
            <field name="name">Beautiful Villa</field>
            <field name="description">A beautiful villa located near the city center.</field>
            <field name="postcode">12345</field>
            <field name="availability_date" eval="datetime.now()"/>
            <field name="expected_price">290000.0</field>
            <field name="bedrooms">4</field>
            <field name="living_area">250</field>
            <field name="facades">2</field>
            <field name="has_garage" eval="True"/>
            <field name="has_garden" eval="True"/>
            <field name="garden_area">50</field>
            <field name="garden_orientation">south</field>
            <field name="code">EPT00001</field>
        </record>
        <record id="demo_estate_property_2" model="estate.property">
            <field name="name">Luxury Penthouse</field>
            <field name="description">A luxurious penthouse with stunning city views and modern amenities.</field>
            <field name="postcode">67890</field>
            <field name="availability_date" eval="datetime.now()"/>
            <field name="expected_price">480000.0</field>
            <field name="has_garage" eval="True"/>
            <field name="garden_area">0</field>
            <field name="garden_orientation">east</field>
            <field name="code">EPT00002</field>
        </record>
        <record id="demo_estate_property_3" model="estate.property">
            <field name="name">Modern Apartment</field>
            <field name="description">A modern apartment with open spaces and beautiful interiors.</field>
            <field name="postcode">12345</field>
            <field name="availability_date" eval="datetime.now()"/>
            <field name="expected_price">350000.0</field>
            <field name="has_garage" eval="True"/>
            <field name="garden_area">20</field>
            <field name="garden_orientation">west</field>
            <field name="is_active" eval="True"/>
            <field name="stage">new</field>
            <field name="code">EPT00003</field>
        </record>

        <record id="demo_estate_property_4" model="estate.property">
            <field name="name">Urban Studio</field>
            <field name="description">Compact studio in the heart of the city, ideal for singles or couples.</field>
            <field name="postcode">54321</field>
            <field name="availability_date" eval="datetime.now()"/>
            <field name="expected_price">220000.0</field>
            <field name="has_garage" eval="False"/>
            <field name="garden_area">0</field>
            <field name="garden_orientation">north</field>
            <field name="is_active" eval="True"/>
            <field name="stage">offer_received</field>
            <field name="code">EPT00004</field>
        </record>

        <record id="demo_estate_property_5" model="estate.property">
            <field name="name">Countryside Villa</field>
            <field name="description">Spacious villa with a beautiful garden and peaceful surroundings.</field>
            <field name="postcode">67892</field>
            <field name="availability_date" eval="datetime.now()"/>
            <field name="expected_price">750000.0</field>
            <field name="has_garage" eval="True"/>
            <field name="garden_area">150</field>
            <field name="garden_orientation">south</field>
            <field name="is_active" eval="False"/>
            <field name="stage">offer_accepted</field>
            <field name="code">EPT00005</field>
        </record>
    </data>
    <data>
        <record id="demo_estate_property_type_1" model="estate.property.type">
            <field name="name">Residential</field>
        </record>
        <record id="demo_estate_property_type_2" model="estate.property.type">
            <field name="name">Commercial</field>
        </record>
        <record id="demo_estate_property_type_3" model="estate.property.type">
            <field name="name">Industrial</field>
        </record>
        <record id="demo_estate_property_tag_1" model="estate.property.tag">
            <field name="name">Luxury</field>
        </record>
        <record id="demo_estate_property_tag_2" model="estate.property.tag">
            <field name="name">Affordable</field>
        </record>
        <record id="demo_estate_property_tag_3" model="estate.property.tag">
            <field name="name">New</field>
        </record>
    </data>


</odoo>