<odoo>
    <data>
        <template id="custom_assets_primary_variables" inherit_id="web._assets_primary_variables">
            <xpath expr="." position="inside">
                <link rel="stylesheet" type="text/scss" href="/branding/static/src/scss/change_color_primary.scss"/>
            </xpath>
        </template>
    </data>

    <template id="assets_backend" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/scss" href="/branding/static/src/scss/custom_navbar.scss"/>
        </xpath>
    </template>

    <template id="assets_frontend"  inherit_id="web.assets_frontend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/css" href="/branding/static/src/scss/home_page.scss"/>
            <link rel="stylesheet" type="text/css" href="/branding/static/src/scss/feedback_form.scss"/>
        </xpath>
    </template>
</odoo>